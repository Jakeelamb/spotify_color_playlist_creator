# Create .gitignore file
cat > .gitignore << 'EOF'
# Python bytecode
__pycache__/
*.py[cod]
*$py.class

# Distribution / packaging
dist/
build/
*.egg-info/

# Environment
.env
.venv
env/
venv/
ENV/
env.bak/
venv.bak/

# Cache files
*.json

# Generated images
*_mosaic.jpg

# IDE files
.idea/
.vscode/
*.swp
*.swo

# Logs
*.log
EOF